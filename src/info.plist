				<key>applescript</key>
				<string>on alfred_script(q)	tell application "iTunes"		if exists current track then			set currentTrackName to name of current track			if loved of the current track is true then				set loved of current track to false				display notification "‚Äú" &amp; currentTrackName &amp; "‚Äù unloved ‚ô°."			else				set loved of the current track to true				display notification "‚Äú" &amp; currentTrackName &amp; "‚Äù loved ‚ô•."			end if		else			display notification "No track is currently playing."		end if	end tellend alfred_script</string>
